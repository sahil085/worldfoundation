<script>
    var p=0;
    $(document).ready(function () {

        var obj = $('#pagination').twbsPagination({
            totalPages: 10,
            visiblePages: 5,
            onPageClick: function (event, page) {
                $("#harinaamsankirtanrequests").html('');
                $.ajax({
                    url: 'harinaamAdminPagePagination?index=' + page,
                    method: "get",
                    success: function (response) {
                        $('#harinaamsankirtanrequests').append(response);
                    }
                })
             p=page;
            }


        })
    });

    function useDefinedPagination() {
        $.ajax({
            url: 'harinaamAdminPagePagination?index=' + p,
            method: "get",
            success: function (response) {
                $("#harinaamsankirtanrequests").html('');
                $('#harinaamsankirtanrequests').append(response);
            }
        })

    }

    function approveRequest(e,id) {
     $(e.target).button('loading');
     document.getElementById(id).disabled=true;
        $.ajax({
            url: 'harinaamSankirtanRequestApprove?id=' + id,
            method: "Get",
            success: function (response) {
                setTimeout(function() {
                    $.bootstrapGrowl(response, { type: 'success',position:'top-right' });
                    }, 1000);
                useDefinedPagination();
            }
        })
    }

    function declineRequest(e,id) {
        $(e.target).button('loading');
        document.getElementById(id).disabled=true;
        $.ajax({
            url: 'harinaamSankirtanRequestDecline?id=' + id,
            method: "Get",
            success: function (response) {
                setTimeout(function() {
                    $.bootstrapGrowl(response, { type: 'danger',position:'top-right' });
                }, 1000);
               useDefinedPagination();
            }
        })
    }

</script>
